local CheckKey = Instance.new("TextButton")
CheckKey.Size = UDim2.new(0.35, 0, 0.15, 0)
CheckKey.Position = UDim2.new(0.55, 0, 0.7, 0)
CheckKey.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
CheckKey.Text = "检查密钥"
CheckKey.TextSize = 18
CheckKey.TextColor3 = Color3.fromRGB(150, 150, 150)
CheckKey.Parent = Frame

local CheckKeyCorner = Instance.new("UICorner")
CheckKeyCorner.CornerRadius = UDim.new(0, 5)
CheckKeyCorner.Parent = CheckKey

GetKey.MouseButton1Click:Connect(function()
   setclipboard("https://work.ink/1U4J/ltkw87e1")
end)

local function validateKey(key)
    return key == "f51373dc-1349-418d-a29d-76db7cca3100"
end

CheckKey.MouseButton1Click:Connect(function()
    local enteredKey = TextBox.Text
    if validateKey(enteredKey) then
        TextBox.PlaceholderText = "密钥正确!"
        TextBox.Text = ""
        wait(1)
        ScreenGui:Destroy()

loadstring(game:HttpGet("https://raw.githubusercontent.com/munciseek/DOORS-script-Mode/main/Source-code"))()

    else
        TextBox.PlaceholderText = "无效密钥"
        TextBox.Text = ""
        wait(1)
        TextBox.PlaceholderText = "确定密钥"
        TextBox.Text = ""
